<div class="pull-right"><%= link_to "Add Image", new_post_path %></div>
<% @posts.each do |post|%>
<div class="posts-wrapper row">
  <div class="post">
      <div class="post-head">
        <div class="name"><%=post.user.user_name%></div>
      </div>
      <div class="image center-block">
        <%= link_to (image_tag post.image.url(:medium)), post_path(post)%>
      </div>
      <p class="caption"><%= post.caption%></p>
  </div>
</div>
<% end %>
