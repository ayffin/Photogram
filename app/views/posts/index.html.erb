<% @posts.each do |post|%>
<div class="posts-wrapper row">
  <div class="post">
      <div class="post-head">
        <div class="name">Ayffi</div>
      </div>
      <div class="image center-block">
        <%= image_tag post.image.url(:medium)%>
      </div>
      <p class="caption"><%= post.caption%></p>
  </div>
</div>
<% end %>
